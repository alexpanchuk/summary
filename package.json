{
  "name": "start-nodejs",
  "version": "1.0.0",
  "description": "Nodejs configuring new project",
  "main": "index.js",
  "author": "Alex Panchuk <apanchuk@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "rimraf ./dist && babel -d ./dist ./src -s && node ./dist/server.js",
    "dev": "nodemon dev.js",
    "seed": "babel-node src/seeds",
    "lint:js": "eslint ./src",
    "precommit": "eslint ./src"
  },
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.0.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "husky": "^0.14.3",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "config": "^1.28.1",
    "dotenv": "^4.0.0",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.1.0",
    "keymirror": "^0.1.1",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.1.0",
    "koa-router": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.4",
    "mongoose-unique-validator": "^1.0.6",
    "rimraf": "^2.6.2"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "func-names": 0,
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "linebreak-style": 0
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
